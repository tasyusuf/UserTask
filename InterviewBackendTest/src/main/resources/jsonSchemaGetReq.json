{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": [
    "status",
    "id",
    "name",
    "email",
    "gender"
  ],
  "properties": {
    "status": {
      "type": "string",
      "default": "",
      "title": "The status Schema",
      "examples": [
        "inactive"
      ]
    },
    "id": {
      "type": "integer",
      "default": 0,
      "title": "The id Schema",
      "examples": [
        3222
      ]
    },
    "name": {
      "type": "string",
      "default": "",
      "title": "The name Schema",
      "examples": [
        "Gino Paloma"
      ]
    },
    "email": {
      "type": "string",
      "default": "",
      "title": "The email Schema",
      "examples": [
        "qatest9@test.com"
      ]
    },
    "gender": {
      "type": "string",
      "default": "",
      "title": "The gender Schema",
      "examples": [
        "male"
      ]
    }
  },
  "examples": [{
    "status": "inactive",
    "id": 3222,
    "name": "Gino Paloma",
    "email": "qatest9@test.com",
    "gender": "male"
  }]
}